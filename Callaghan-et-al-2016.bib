  author = {Duguay, {Claude} {R}. and {Flato}, {Greg} {M}. and {Jeffries}, {Martin} {O}. and {M}{\'e}nard, {Patrick} and {Morris}, {Kim} and {Rouse}, {Wayne} {R}.},
  month = dec,
  year = {2003},
  keywords = {forest,lake ice,modelling,shallow lakes,snow,tundra},
  pages = {3465--3483},
  file = {Duguay et al 2003 ice depth.pdf:/Users/DavieCal/Documents/Zotero_Papers/storage/CG5XX4DN/Duguay et al 2003 ice depth.pdf:application/pdf;Snapshot:/Users/DavieCal/Documents/Zotero_Papers/storage/SZATX4IT/abstract.html:}
