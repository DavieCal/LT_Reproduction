@article{gross_alternative_1996,
  title = {Alternative reproductive strategies and tactics: diversity within sexes},
  volume = {11},
  issn = {0169-5347},
  shorttitle = {Alternative reproductive strategies and tactics},
  url = {http://www.sciencedirect.com/science/article/pii/0169534796810500},
  doi = {10.1016/0169-5347(96)81050-0},
  abstract = {Not all members of a sex behave in the same way. Frequency- and statusdependent selection have given rise to many alternative reproductive phenotypes within the sexes. The evolution and proximate control of these alternatives are only beginning to be understood. Although game theory has provided a theoretical framework, the concept of the mixed strategy has not been realized in nature, and alternative strategies are very rare. Recent findings suggest that almost all alternative reproductive phenotypes within the sexes are due to alternative tactics within a conditional strategy, and, as such, while the average fitnesses of the alternative phenotypes are unequal, the strategy is favoured in evolution. Proximate mechanisms that underlie alternative phenotypes may have many similarities with those operating between the sexes.},
  timestamp = {2013-08-11T02:39:08Z},
  number = {2},
  urldate = {2013-08-11},
  journal = {Trends in {Ecology} \& {Evolution}},
  author = {Gross, {Mart} {R}.},
