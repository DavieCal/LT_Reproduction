  journal = {Conservation {Letters}},
  author = {Moore, {Jonathan} {W}. and {McClure}, {Michelle} and {Rogers}, {Lauren} {A}. and {Schindler}, {Daniel} {E}.},
  month = sep,
  year = {2010},
  keywords = {Biocomplexity,Chinook,coherence,portfolio effect,response diversity},
  pages = {340--348},
  file = {Snapshot:/Users/DavieCal/Documents/Zotero_Papers/storage/E9MQGVUS/abstract.html:;portfolio effect.pdf:/Users/DavieCal/Documents/Zotero_Papers/storage/KRXGHK7M/portfolio effect.pdf:application/pdf}
}

@article{kenney_physical_1996,
  title = {Physical limnological processes under ice},
  volume = {322},
  issn = {0018-8158, 1573-5117},
  url = {http://link.springer.com.proxy2.lib.umanitoba.ca/article/10.1007/BF00031810},
  doi = {10.1007/BF00031810},
  abstract = {Penetration of solar radiation through ice and snow covering northern lakes produces a gravity current between regions with varying depths. This baroclinic current is a dominant physical process in winter because ice cover insulates lakes from the usual turbulence sources such as breaking surface waves and near-surface shear produced by the wind. The current forms a directed circulation from the littoral zone to the centre of the lake that is an important distribution mechanism for nutrients and other chemical and biological constituents. Heat transported by the current degrades the ice cover and makes surface travel hazardous. The thinning of the ice cover is most severe at the inlet to isolated bays with mean depths that differ significantly from the lake. At the mouth of a bay, the gravity current takes the form of a two-layer flow with inflow to the bay occurring near the surface. The lower layer has the largest temperature gradients and is dominated by a succession of progressive internal bores which decrease in amplitude overnight and with increasing cloud cover. The repetition of the bores occurs very close to the period of the uninodal barotropic seiche which suggests that the internal bores are forced by the surface seiche.},
  language = {en},
  timestamp = {2015-02-23T17:28:40Z},
  number = {1-3},
  urldate = {2015-02-23},
  journal = {Hydrobiologia},
  author = {Kenney, {Bernard} {C}.},
  month = apr,
  year = {1996},
  keywords = {baroclinic circulation,Ecology,Hydrobiology,internal bores,under-ice currents,winter limnology},
  pages = {85--90},
  file = {Snapshot:/Users/DavieCal/Documents/Zotero_Papers/storage/J4CMJS6G/BF00031810.html:;Full Text PDF:/Users/DavieCal/Documents/Zotero_Papers/storage/JUZ95MTP/Kenney - 1996 - Physical limnological processes under ice.pdf:application/pdf}
